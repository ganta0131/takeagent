{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.11",
        "installCommand": "pip install -r requirements.txt",
        "launchCommand": "python app.py"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "networkAccess": {
    "allowed": [
      "*.googleapis.com",
      "*.gstatic.com",
      "api.openweathermap.org"
    ]
  }
}
